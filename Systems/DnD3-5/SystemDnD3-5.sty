\ForLoop{@i}{0}{\value{@i}<10}{
	\NewCounter{charAbilitySpellSlotsOne\arabic{@i}Count}
	\NewCounter{charAbilitySpellSlotsTwo\arabic{@i}Count}
	\NewCounter{charClassSpellSlotsOne\arabic{@i}Count}
